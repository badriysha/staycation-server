<!-- DataTales Example -->
<div
  class="tab-pane fade show active"
  id="home"
  role="tabpanel"
  aria-labelledby="show-image"
>
  <div class="card shadow mb-4">
    <div class="card-body">
      <div class="table-responsive">
        <div class="row">
          <% for(let i = 0; i < item.imageId.length; i++){ %>
          <div class="col-auto">
            <img
              src="http://localhost:3000/<%= item.imageId[i].imageUrl %>"
              alt="image item"
              style="display: block; width: 200px; height: 200px"
            />
          </div>
          <% } %>
        </div>
      </div>
      <div class="my-3">
        <a href="/admin/item" class="btn btn-sm btn-primary px-3">Back</a>
      </div>
    </div>
  </div>
</div>
